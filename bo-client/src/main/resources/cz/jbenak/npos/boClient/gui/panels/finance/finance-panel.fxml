<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>

<BorderPane fx:id="mainPane" prefHeight="768.0" prefWidth="1260.0" styleClass="panel" stylesheets="@../../styles/panels.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.jbenak.npos.boClient.gui.panels.finance.FinancePanelController">
    <top>
        <BorderPane fx:id="menuPane" prefHeight="9.0" prefWidth="1250.0" BorderPane.alignment="CENTER">
            <center>
            </center>
         <top>
            <FlowPane prefHeight="27.0" prefWidth="1250.0" styleClass="panel-main-menu" BorderPane.alignment="CENTER">
               <children>
                        <Button fx:id="btnMenuDeposits" mnemonicParsing="false" onAction="#btnMenuDepositsPressed" text="Vklady">
                     <FlowPane.margin>
                        <Insets />
                     </FlowPane.margin></Button>
                        <Button fx:id="btnMenuDisbursements" mnemonicParsing="false" onAction="#btnMenuDisbursementsPressed" text="Výběry">
                     <FlowPane.margin>
                        <Insets left="5.0" />
                     </FlowPane.margin></Button>
                        <Button fx:id="btnMenuViews" mnemonicParsing="false" onAction="#btnMenuViewsPressed" text="Přehledy">
                     <FlowPane.margin>
                        <Insets left="5.0" />
                     </FlowPane.margin></Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" />
               </BorderPane.margin>
            </FlowPane>
         </top>
        </BorderPane>
    </top>
    <padding>
        <Insets bottom="10.0" right="10.0" top="10.0" />
    </padding>
</BorderPane>
